{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-hooks",
        "prettier"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "camelcase": "off",
        "spaced-comment": "error",
        "quotes": [
            "error",
            "single"
        ],
        "no-duplicate-imports": "error",
        "no-unused-vars": "warn",
        "react/prop-types": 0,
        "react/no-unescaped-entities": 0,
        "react/no-children-prop": 0,
        "react/display-name": 0
    }
}